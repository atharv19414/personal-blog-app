<%- include('partials/header'); -%>

    <h1>Home</h1>

    <p>
        <%= homeContent %>
    </p>

    <% posts.forEach(element=> {%>
        <h1>
            <%= element.title %>
        </h1>

        <p>
            <%= element.post.substring(0,99)+ " ..."%>
                <a href="/posts/<%=element._id %>">Read More</a>
        </p>
        <% }); %>

            <%- include('partials/footer'); -%>